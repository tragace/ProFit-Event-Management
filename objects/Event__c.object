    <validationRules>
        <fullName>End_DateTime_must_be_atleast_1_day_ahead</fullName>
        <active>true</active>
        <description>End Date/Time must be at-least 1 day ahead of Start Date/Time</description>
        <errorConditionFormula>AND( DATEVALUE( End_Date_Time__c ) &lt;  DATEVALUE( Start_Date_Time__c ) + 1 , NOT( ISNULL(End_Date_Time__c ) ))</errorConditionFormula>
        <errorDisplayField>End_Date_Time__c</errorDisplayField>
        <errorMessage>End Date/Time must be at-least 1 day ahead of Start Date/Time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Event_Type_In_Person</fullName>
        <active>true</active>
        <description>If Event Type field value is In-Person then user must need to select Location on Event Record.</description>
        <errorConditionFormula>AND( ISPICKVAL( Event_Type__c , &apos;In-person&apos;) ,   (ISBLANK( Address_Book__c ) ) )</errorConditionFormula>
        <errorDisplayField>Address_Book__c</errorDisplayField>
        <errorMessage>Event Type is In Person. Please input an Address.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Recurring_Validation_Rule_If_Not_Checked</fullName>
        <active>true</active>
        <description>If Recurring? checkbox is checked then user must need to fill Frequency field.</description>
        <errorConditionFormula>AND(
Recurring__c = False,
NOT(ISBLANK(TEXT( Frequency__c )))
)</errorConditionFormula>
        <errorDisplayField>Recurring__c</errorDisplayField>
        <errorMessage>Recurring not checked. Cannot select Frequency.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Virtual_Value_Selected</fullName>
        <active>true</active>
        <description>If Virtual is Selected as Value for Event Type field then Prevent User to Select Location on Event Record.</description>
        <errorConditionFormula>AND( ISPICKVAL( Event_Type__c , &apos;Virtual&apos;) ,   NOT(ISBLANK( Address_Book__c ) ) )</errorConditionFormula>
        <errorDisplayField>Address_Book__c</errorDisplayField>
        <errorMessage>The Event Type is Virtual. You cannot set an Address.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
